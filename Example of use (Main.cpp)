#include "String.h"
#include <iostream>

int main()
{
	String str;
	str = "have ";
	str += "a cat ";
	str = str + "and a dog";
	str = "I " + str;
	std::cout << str.toStandard() << '\n'; //prints I have a cat and a dog

	str -= " and a dog";
	std::cout << str.toStandard(); //prints I have a cat
}
